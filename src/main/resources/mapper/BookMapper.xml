            <result property="book_isbn" column="book_isbn" jdbcType="VARCHAR"/>
            <result property="book_page" column="book_page" jdbcType="VARCHAR"/>
            <result property="book_format" column="book_format" jdbcType="INTEGER"/>
            <result property="book_introductory" column="book_introductory" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        book_id,book_name,book_author,
        book_price,book_stock,publishhouse_id,
        book_deleted,book_isbn,book_page,
        book_format,book_introductory
